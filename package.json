{
  "name": "ew-user-registry-lib",
  "version": "1.0.0",
  "description": "",
  "main": "build/ts/index.js",
  "types": "build/ts/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start-test-backend": "node node_modules/ewf-coo-test-backend/index.js",
    "test": "npm run build-and-deploy && mocha build/ts/test/",
    "start-ganache": "ganache-cli -m 'chalk park staff buzz chair purchase wise oak receive avoid avoid home' -g 0 -l 80000000 -e 1000000 -a 20",
    "testlight": "npm run build-ts && mocha build/ts/test/",
    "debug-light-test": "npm run build-ts && mocha --inspect-brk build/ts/test/",
    "setup-SS": "npm run build-ts && node build/ts/utils/deployment/secretStore",
    "build-and-deploy": "npm run build && npm run deploy-contracts",
    "deploy-contracts": "node build/ts/utils/deployment/startMigrate",
    "build": "npm run compile-contracts && npm run build-ts && npm run build-schemas",
    "precompile-contracts": "rm -rf build/contracts",
    "compile-contracts": "node compile.js",
    "prebuild-ts": "rm -rf build/ts",
    "build-ts": "node_modules/typescript/bin/tsc && cp src/types/*.d.ts build/ts/types/",
    "prebuild-schemas": "rm -rf build/schemas && mkdir build/schemas",
    "build-schemas": "npm run build-schema:AssetPropertiesOffChain && npm run build-schema:ProducingAssetPropertiesOffChain",
    "build-schema:AssetPropertiesOffChain": "typescript-json-schema --ignoreErrors --required src/blockchain-facade/Asset/Asset.ts OffChainProperties > build/schemas/AssetPropertiesOffChain.schema.json",
    "build-schema:ProducingAssetPropertiesOffChain": "typescript-json-schema --ignoreErrors --required src/blockchain-facade/Asset/ProducingAsset.ts OffChainProperties > build/schemas/ProducingAssetPropertiesOffChain.schema.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.slock.it/ewf/origin/user-registry/ew-user-registry-lib.git"
  },
  "Dependencies": {
    "remixd": "^0.1.2"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://git.slock.it/ewf/origin/user-registry/ew-user-registry-lib"
  },
  "homepage": "https://git.slock.it/ewf/origin/user-registry/ew-user-registry-lib#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "curlrequest": "^1.0.1",
    "ethereumjs-tx": "^1.3.7",
    "ew-asset-registry-lib": "0.0.1",
    "jsonschema": "^1.2.4",
    "precise-proofs": "git+https://github.com/slockit/precise-proofs.git",
    "sloffle": "v1.1.2",
    "web3": "^1.0.0-beta.34",
    "winston": "^3.0.0",
    "write-json-file": "^2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.6.28",
    "chai": "^4.1.2",
    "ethers": "^3.0.17",
    "ewf-coo-test-backend": "git+https://github.com/slockit/ewf-coo-test-backend.git",
    "ganache-cli": "^6.1.8",
    "mocha": "^5.2.0",
    "solc": "^0.4.23",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "typescript-json-schema": "^0.31.0"
  }
}