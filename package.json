{
  "name": "ew-user-registry-lib",
  "version": "1.0.0",
  "description": "",
  "main": "dist/js/index.js",
  "types": "dist/js/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "parity-docker": "docker pull docker.slock.it/build-images/parity:v1.11.8-tobalaba && docker run --name 'parityDocker' -p 8180:8180 -p 8545:8545 -p 8546:8546 -p 8090:8091 docker.slock.it/build-images/parity:v1.11.8-tobalaba",
    "dockerstop": "docker kill 'parityDocker' && docker container rm 'parityDocker'",
    "start-test-backend": "node node_modules/ewf-coo-test-backend/index.js",
    "test": "npm run build-ts && mocha dist/js/test/ --timeout 60000",
    "start-ganache": "ganache-cli -m 'chalk park staff buzz chair purchase wise oak receive avoid avoid home' -g 0 -l 80000000 -e 1000000 -a 20",
    "testlight": "npm run build-ts && mocha build/ts/test/",
    "debug-light-test": "npm run build-ts && mocha --inspect-brk build/ts/test/",
    "build-and-deploy": "npm run build-ts && npm run deploy-contracts",
    "deploy-contracts": "node dist/js/src/utils/deployment/startDeploy",
    "prepare-wrapping": "rm -rf dist/ts/wrappedContracts && mkdir -p dist/ts/wrappedContracts && node wrapping.js",
    "prebuild-ts": "rm -rf dist/js",
    "build-ts": "rm -rf dist/js && node_modules/typescript/bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/energywebfoundation/ew-user-registry-lib.git"
  },
  "author": "slock.it GmbH; Martin Kuechler, martin.kuechler@slock.it; Heiko Burkhardt, heiko.burkhardt@slock.it",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://git.slock.it/ewf/origin/user-registry/ew-user-registry-lib"
  },
  "homepage": "https://github.com/energywebfoundation/ew-user-registry-lib#readme",
  "dependencies": {
    "ew-user-registry-contracts": "0.0.99",
    "ew-utils-general-lib": "0.0.100",
    "web3": "1.0.0-beta.37",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.6.28",
    "@types/web3": "1.0.14",
    "chai": "^4.1.2",
    "ganache-cli": "^6.1.8",
    "mocha": "^5.2.0",
    "typescript": "^2.9.2",
    "typescript-json-schema": "^0.31.0"
  },
  "postinstall": "rm -f node_modules/web3/index.d.ts"
}
